[run]
source = src/
omit = 
    src/**/*.test.ts
    src/**/*.test.tsx
    src/**/*.spec.ts
    src/**/*.spec.tsx
    src/tests/**
    src/**/*.stories.ts
    src/**/*.stories.tsx
    src/components/ui/**
    src/app/layout.tsx

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.: